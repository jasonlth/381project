<HTML>
  <BODY>
    <H1>Update Restaurant</H1>
    <%= e %> <br>
    <form action="/Update" method="post" enctype="multipart/form-data">
      Restaurant Name: <input type="text" name = "name" value = "<%= r.name %>"><br>
      Borough: <input type="text" name = "borough" value = "<%= r.borough %>"><br>
      Cuisine:<input type="text" name = "cuisine" value = "<%= r.cuisine %>"><br>
      Address:<br>
      Street:<input type="text" name="street" value="<%= r.address.street %>"><br>
      Building:<input type="text" name="building" value="<%= r.address.building %>"><br>
      Zipcode:<input type="text" name="zipcode" value="<%= r.address.zipcode %>"><br>
      Coord:<br>
      lon.:<input type="text" name="lon" value="<%= r.address.coord[0] %>">lat.:<input type="text" name="lat" value="<%= r.address.coord[1] %>"><br>
      Photo:<input type="file" name="filebit"><br>
      <input type="hidden" name = "bit" value = "<%= r.image.bit %>"> <br>
      <input type="hidden" name = "type" value = "<%=r.image.type %>"> <br>
      <input type="hidden" name="owner" value= "<%= r.owner %>" ><br>
      <input type="hidden" name="rid" value="<%= r.rid %>"><br>
      <input type="hidden" name="grade" value="<%= g %>"><br>
      <input type="submit" value="Update"><br>
    </form>
    <a href="/home">Back to Home</a>
  </BODY>
</HTML>
